% Simple is good right?
\newcommand{\twopic}[3]{
    \begin{figure}[h!]
        \begin{subfigure}[h!]{0.5\textwidth}
            \includegraphics[width=\textwidth]{#1}
        \end{subfigure}
        ~
        \begin{subfigure}[h!]{0.5\textwidth}
            \includegraphics[width=\textwidth]{#2}
        \end{subfigure}
        \caption{#3}
    \end{figure}
}

\newcommand{\twodiffpic}[4]{
    \begin{figure}[h!]
    \centering
    \begin{minipage}{.5\textwidth}
        \centering
        \includegraphics[width=\linewidth]{#1}
        \captionof{figure}{#2}
    \end{minipage}%
    \begin{minipage}{.5\textwidth}
        \centering
        \includegraphics[width=\linewidth]{#3}
        \captionof{figure}{#4}
    \end{minipage}
    \end{figure}
}

\newcommand{\twodiffpiclabel}[6]{
    \begin{figure}[h!]
    \centering
    \begin{minipage}{.5\textwidth}
        \centering
        \includegraphics[width=\linewidth]{#1}
        \captionof{figure}{#2}
        \label{#3}
    \end{minipage}%
    \begin{minipage}{.5\textwidth}
        \centering
        \includegraphics[width=\linewidth]{#4}
        \captionof{figure}{#5}
        \label{#6}
    \end{minipage}
    \end{figure}
}

